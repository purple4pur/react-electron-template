{
  "name": "react-electron-templete",
  "author": "Purple4pur",
  "private": true,
  "homepage": "./",
  "main": "main.js",
  "dependencies": {},
  "devDependencies": {
    "electron": "^8.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "preelt-dev": "react-scripts build",
    "preelt-pack-win": "react-scripts build",
    "elt-dev": "electron .",
