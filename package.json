{
  "name": "react-electron-template",
  "author": "Purple4pur",
  "private": true,
  "homepage": "./",
  "main": "main.js",
  "dependencies": {
    "electron": "^11.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "pview": "electron .",
    "pack-win": "electron-packager . MyReactElectronApp --asar --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --ignore=^/node_modules$ --ignore=^/src$ --ignore=^/public$ --ignore=^/package-lock.json$",
    "prepview": "react-scripts build",
    "prepack-win": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
